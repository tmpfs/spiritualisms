{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "databases",
      "host": {
        "sourcePath": "/usr/local/var/lib/couchdb"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "www.spiritualisms.org",
      "image": "spiritualisms-www",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "cache.spiritualisms.org",
        "db.spiritualisms.org"
      ],
      "mountPoints": [
        {
          "sourceVolume": "www.spiritualisms.org",
          "containerPath": "/var/www/html",
          "readOnly": true
        }
      ]
    },
    {
      "name": "api.spiritualisms.org",
      "image": "spiritualisms-api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3001,
          "containerPort": 3001
        }
      ],
      "links": [
        "cache.spiritualisms.org",
        "db.spiritualisms.org"
      ]
    },
    {
      "name": "file.spiritualisms.org",
      "image": "spiritualisms-file",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3002,
          "containerPort": 3002
        }
      ],
      "links": [
        "cache.spiritualisms.org",
        "db.spiritualisms.org"
      ]
    },
    {
      "name": "cache.spiritualisms.org",
      "image": "spiritualisms-redis",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    },
    {
      "name": "db.spiritualisms.org",
      "image": "spiritualisms-couchdb",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 5984,
          "containerPort": 5984
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "databases",
          "containerPath": "/usr/local/var/lib/couchdb"
        }
      ]
    },
    {
      "name": "nginx",
      "image": "spiritualisms-nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "www.spiritualisms.org",
        "api.spiritualisms.org",
        "file.spiritualisms.org"
      ],
      "mountPoints": []
    }
  ]
}
