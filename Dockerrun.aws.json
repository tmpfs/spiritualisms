{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "www-app",
      "host": {
        "sourcePath": "/var/app/current/www"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "www-app",
      "image": "spiritualisms-www",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "redis",
        "couchdb"
      ],
      "mountPoints": [
        {
          "sourceVolume": "www-app",
          "containerPath": "/var/www/html",
          "readOnly": true
        }
      ]
    },
    {
      "name": "api-app",
      "image": "spiritualisms-api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3001,
          "containerPort": 3001
        }
      ],
      "links": [
        "redis",
        "couchdb"
      ]
    },
    {
      "name": "file-app",
      "image": "spiritualisms-file",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3002,
          "containerPort": 3002
        }
      ],
      "links": [
        "redis",
        "couchdb"
      ]
    },
    {
      "name": "redis",
      "image": "spiritualisms-redis",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    },
    {
      "name": "couchdb",
      "image": "spiritualisms-couchdb",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 5984,
          "containerPort": 5984
        }
      ]
    },
    {
      "name": "nginx",
      "image": "spiritualisms-nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "www-app",
        "api-app"
      ],
      "mountPoints": [
        {
          "sourceVolume": "www-app",
          "containerPath": "/var/www/html",
          "readOnly": true
        },
        {
          "sourceVolume": "awseb-logs-nginx-proxy",
          "containerPath": "/var/log/nginx"
        }
      ]
    }
  ]
}
